# /etc/logrotate.d/rsyslog

/var/log/dhclient.log
/var/log/messages
{
        rotate 9
        missingok
        notifempty
        size 1M
        delaycompress
        compress
}

/var/log/console.log
{
        rotate 9
        missingok
        notifempty
        size 20M
        delaycompress
        compress
}
